<h1>Listing auto_modes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @auto_modes.each do |auto_mode| %>
      <tr>
        <td><%= auto_mode.title %></td>
        <td><%= link_to 'Show', auto_mode %></td>
        <td><%= link_to 'Edit', edit_auto_mode_path(auto_mode) %></td>
        <td><%= link_to 'Destroy', auto_mode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auto mode', new_auto_mode_path %>
